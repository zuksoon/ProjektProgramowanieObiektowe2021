<Window x:Class="EKantor.Wyplac"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:EKantor"
        mc:Ignorable="d"
        Title="Wyplac" Height="300" Width="500">
    <Grid>
        <Label Content="Obecnie wolnych środków" HorizontalAlignment="Left" Margin="60,71,0,0" VerticalAlignment="Top" FontSize="16"/>
        <TextBox x:Name="TextBoxWolneSrodki" HorizontalAlignment="Left" Margin="260,75,0,0" Text="TextBox" TextWrapping="Wrap" VerticalAlignment="Top" Width="120" FontSize="16"/>
        <Label Content="Ile chcesz wypłacić?" HorizontalAlignment="Left" Margin="158,120,0,0" VerticalAlignment="Top" FontSize="16"/>
        <TextBox x:Name="TextBoxIleWyplacic" HorizontalAlignment="Left" Margin="172,156,0,0" TextWrapping="Wrap" VerticalAlignment="Top" Width="120" FontSize="16"/>
        <Button x:Name="btnOK" Content="OK" HorizontalAlignment="Left" Margin="346,212,0,0" VerticalAlignment="Top" FontSize="18" Width="80" Click="btnOK_Click"/>
        <Button x:Name="btnAnuluj" Content="Anuluj" HorizontalAlignment="Left" Margin="78,212,0,0" VerticalAlignment="Top" FontSize="18" Width="80" Click="btnAnuluj_Click"/>

    </Grid>
</Window>
